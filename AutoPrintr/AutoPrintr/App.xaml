<Application x:Class="AutoPrintr.App"
             x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:system="clr-namespace:System;assembly=mscorlib">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <system:String x:Key="SupportEmailAddress">help@repairshopr.com</system:String>
            <system:String x:Key="SupportEmailSubject">Support request from AutoPrintr</system:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>